    "next",
    "cms",
    "headless",
    "react"
  ],
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0"
  },
  "author": "gabriel-miranda <gabmirdev@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "contentful": "^7.0.5",
    "debug-logger": "^0.4.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "lru-cache": "^4.1.3",
    "next": "^7.0.2",
    "prop-types": "^15.6.2",
